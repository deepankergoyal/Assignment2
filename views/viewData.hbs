<h1>Sales Data</h1>
<table>
  <thead>
    <tr>
      <th>Invoice ID</th>
      <th>Branch</th>
      <th>City</th>
      <th>Customer Type</th>
      <th>Product Line</th>
      <th>Name</th>
      <th>Image</th>
      <th>Unit Price</th>
      <th>Quantity</th>
      <th>Tax 5%</th>
      <th>Total</th>

      <th>Payment</th>

      <th>Rating</th>
    </tr>
  </thead>
  <tbody>
    {{#each jsonData}}
      <tr>
        <td>{{"Invoice ID"}}</td>
        <td>{{this.Branch}}</td>
        <td>{{this.City}}</td>
        <td>{{"Customer type"}}</td>
        <td>{{"Product line"}}</td>
        <td>{{this.name}}</td>
        <td><img src="{{this.image}}" alt="{{this.name}}" width="100" /></td>
        <td>{{"Unit price"}}</td>
        <td>{{this.Quantity}}</td>
        <td>{{"Tax 5%"}}</td>

        <td>{{this.Time}}</td>
        <td>{{this.Payment}}</td>

        <td>{{this.Rating}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>